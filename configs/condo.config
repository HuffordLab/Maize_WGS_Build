process {
  withLabel:sra_tools  { module = 'sratools' }
  withLabel:ncbi_blast { module = 'blastplus' }
  withLabel:canu       { module = 'canu' }
  withLabel:gatk0_index { module = 'samtools:picard:bwa:bedtools2:bioawk' }
}